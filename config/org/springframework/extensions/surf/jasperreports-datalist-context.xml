<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>
   
   <!-- Load the datalist form config -->
   <bean id="jasperreportsDatalistFormsClientConfig" class="org.springframework.extensions.config.ConfigBootstrap" init-method="register">
         <property name="configService" ref="web.config" />
         <property name="configs">
            <list>
               <value>classpath:alfresco/web-extension/jasperreports-datalist-config-custom.xml</value>
            </list>
         </property>
   </bean>
   
   <!-- Add custom global messages -->
   <bean id="share-extras.site-jasperreports.resources" class="org.springframework.extensions.surf.util.ResourceBundleBootstrapComponent">
      <property name="resourceBundles">
         <list>
            <value>alfresco.messages.share-site-jasperreports</value>
         </list>
      </property>
   </bean>
   
</beans>